<VirtualHost *:"{{ kibana_proxy_port }}">      
  ProxyPass "/app/kibana" "{{ schema }}://{{ kibana_url }}:{{ kibana_port }}"
  ProxyPassReverse "/app/kibana" "{{ schema }}://{{ kibana_url }}:{{ kibana_port }}"
  ProxyPass "/kibana" "{{ schema }}://{{ kibana_url }}:{{ kibana_port }}"
  ProxyPassReverse "/kibana" "{{ schema }}://{{ kibana_url }}:{{ kibana_port }}"     
</VirtualHost>
